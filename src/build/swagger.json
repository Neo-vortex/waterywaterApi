				"description": "Delete a user by ID",
				"security": [],
				"parameters": [
					{
						"description": "User's ID",
						"in": "path",
						"name": "id",
						"required": true,
						"schema": {
							"format": "double",
							"type": "number"
						}
					}
				]
			}
